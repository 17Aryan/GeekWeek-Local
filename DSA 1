#User function Template for python3

def getSum( arr, n):
    # Your code goes here
    su = 0
    for i in range(n):
        su = su + arr[i]
    return su
    


#{ 
#  Driver Code Starts
#Initial Template for Python 3

def main():

    T = int(input())

    while(T > 0):
        n = int(input())
        a = [int(x) for x in input().strip().split()]
        print(getSum(a, n))

        T -= 1


if __name__ == "__main__":
    main()





    
# } Driver Code Ends
